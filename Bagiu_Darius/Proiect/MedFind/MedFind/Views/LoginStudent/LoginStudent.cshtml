@model MedFind.Models.LoginStudentViewModel

@{
    ViewData["Title"] = "LoginStudent";
}

<h1>Login Student</h1>

<div class="row">
    <div class="col-md-12">
        <form method="post">

            

            <div class="form-group">
                <label asp-for="StudentAccount"></label>
                <input asp-for="StudentAccount" class="form-control" />
                <span asp-validation-for="StudentAccount" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Password"></label>
                <input asp-for="Password" class="form-control" />
                <span asp-validation-for="Password" class="text-danger"></span>
            </div>

            <button type="submit" class="btn btn-primary">Login</button>
        </form>
    </div>
</div>