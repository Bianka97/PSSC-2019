@model MedFind.Models.ViewModels.LoginMedicViewModel
@{
    ViewData["Title"] = "LoginMedic";
}

<h1>Login Medic</h1>

<div class="row">
    <div class="col-md-12">
        <form method="post">

            

            <div class="form-group">
                <label asp-for="MedicAccount" ></label>
                <input asp-for="MedicAccount" class="form-control" />
                <span asp-validation-for="MedicAccount" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Password" ></label>
                <input asp-for="Password" class="form-control" />
                <span asp-validation-for="Password" class="text-danger"></span>
            </div>

            <button type="submit" class="btn btn-primary">Login</button>
        </form>
    </div>
</div>